:root{
  --TitleFont : {{ settings.title__font.family }},{{ settings.title__font.fallback_families }};
  --BodyFont : {{ settings.body__font.family }},{{ settings.body__font.fallback_families }} ;
}
    
 body{
   font-family:var(--BodyFont);
    font-weight: 400;
    font-style: normal;
    font-size: 18px;
    text-transform: none;
    color: #333;
    line-height: 1.6em;
    overflow: auto;
 }

    
    
    
    .full--four{ 
     width:calc(25% - 20px);
      margin:0 auto;
    }
    .full--three{
       width:calc(33% - 20px);
      margin:0 auto;
    }
    .full--five{
       width:calc(20% - 20px);
      margin:0 auto;
    }
    
    @media(min-width:768px){
      .medium--three{
         width:calc(33% - 20px);
      margin:0 auto;
      }
      .medium--two{
        width:calc(50% - 20px);
      margin:0 auto;
        
      }
    }
    
    @media(max-width:767px){
      .small--one{
         width:calc(100% - 20px);
      margin:0 auto;
      }
    }
    
    
    